<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".ActividadActivity">

    <include
        layout="@layout/barra_menu2"
        android:layout_width="match_parent"
        android:layout_height="85dp"></include>

    <Button
        android:id="@+id/borrarBoton"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:layout_marginStart="210dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="76dp"
        android:text="Borrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/modificarBoton"
        android:layout_width="146dp"
        android:layout_height="45dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="229dp"
        android:layout_marginBottom="578dp"
        android:text="Modificar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/descripcionInput"
        android:layout_width="366dp"
        android:layout_height="143dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="376dp"
        android:ems="10"
        android:enabled="false"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/lugarInput"
        android:layout_width="68dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="392dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Lugar"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="61dp"
        android:layout_height="24dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="140dp"
        android:text="Lugar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.349"
        app:layout_constraintStart_toEndOf="@+id/fechaInput"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/horaFinalInput"
        android:layout_width="60dp"
        android:layout_height="21dp"
        android:layout_marginTop="520dp"
        android:layout_marginEnd="168dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Hora inicio"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.242"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/tituloInput"
        android:layout_width="174dp"
        android:layout_height="47dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="129dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:inputType="textPersonName"
        android:shadowColor="#000000"
        android:text="Name"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/horaInicioInput"
        android:layout_width="64dp"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="472dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Hora inicio"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="70dp"
        android:layout_height="23dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="468dp"
        android:text="Hora Inicio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="64dp"
        android:layout_height="23dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="520dp"
        android:text="Hora final"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/guardarBoton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="224dp"
        android:layout_marginTop="592dp"
        android:enabled="true"
        android:text="Guardar"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="388dp"
        android:text="Fecha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/precioInput"
        android:layout_width="62dp"
        android:layout_height="28dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="472dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="precio"
        android:inputType="number|textPersonName"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.234"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/fechaInput"
        android:layout_width="68dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="392dp"
        android:autoText="true"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="fecha"
        android:inputType="textPersonName"
        android:textColor="#090909"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="61dp"
        android:layout_height="29dp"
        android:layout_marginTop="472dp"
        android:layout_marginEnd="88dp"
        android:text="Precio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.214"
        app:layout_constraintStart_toEndOf="@+id/horaInicioInput"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/apuntarseBoton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apuntarse"
        app:rippleColor="#00FFFFFF"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="528dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="-11dp" />

    <androidx.constraintlayout.helper.widget.Flow
        android:id="@+id/flow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="37dp" />

</androidx.constraintlayout.widget.ConstraintLayout>