<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".ActividadActivity">

    <Button
        android:id="@+id/guardarBoton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="280dp"
        android:layout_marginTop="540dp"
        android:enabled="true"
        android:text="Guardar"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/horaFinalInput"
        android:layout_width="107dp"
        android:layout_height="36dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="452dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Hora inicio"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="86dp"
        android:layout_height="34dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="452dp"
        android:text="Hora final"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/borrarBoton"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="73dp"
        android:text="Borrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/modificarBoton"
        android:layout_width="146dp"
        android:layout_height="45dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="16dp"
        android:text="Modificar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/precioInput"
        android:layout_width="83dp"
        android:layout_height="31dp"
        android:layout_marginStart="324dp"
        android:layout_marginTop="388dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="precio"
        android:inputType="number|textPersonName"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/horaInicioInput"
        android:layout_width="107dp"
        android:layout_height="36dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="392dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Hora inicio"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/lugarInput"
        android:layout_width="115dp"
        android:layout_height="30dp"
        android:layout_marginStart="280dp"
        android:layout_marginTop="320dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Lugar"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/fechaInput"
        android:layout_width="99dp"
        android:layout_height="32dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="320dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:hint="fecha"
        android:inputType="textPersonName"
        android:textColor="#090909"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="70dp"
        android:layout_height="34dp"
        android:layout_marginTop="388dp"
        android:layout_marginEnd="92dp"
        android:text="Precio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="322dp"
        android:text="Fecha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="91dp"
        android:layout_height="39dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="392dp"
        android:text="Hora Inicio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="62dp"
        android:layout_height="27dp"
        android:layout_marginTop="322dp"
        android:layout_marginEnd="151dp"
        android:text="Lugar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/tituloInput"
        android:layout_width="174dp"
        android:layout_height="47dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="76dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:enabled="false"
        android:gravity="center_horizontal|center_vertical"
        android:inputType="textPersonName"
        android:shadowColor="#000000"
        android:text="Name"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/descripcionInput"
        android:layout_width="366dp"
        android:layout_height="143dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="137dp"
        android:ems="10"
        android:enabled="false"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/apuntarseBoton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="260dp"
        android:layout_marginTop="452dp"
        android:text="Apuntarse"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#00FFFFFF" />

</androidx.constraintlayout.widget.ConstraintLayout>